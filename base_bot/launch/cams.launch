<?xml version="1.0"?>
<launch>
   <!-- launch video stream -->
   <include file="$(find video_stream_opencv)/launch/camera.launch" >
   		<!-- node name and ros graph name -->
	  	<arg name="camera_name" value="right_cam" />
	  	<!-- means video device 0, /dev/video0 -->
	  	<arg name="video_stream_provider" value="$(arg video_stream_provider_right)" />
	  	<!-- set camera fps to (if the device allows) -->
	  	<arg name="set_camera_fps" value="15"/>
      		<!-- set buffer queue size of frame capturing to (1 means we want the latest frame only) -->
      		<arg name="buffer_queue_size" value="1" />
	  	<!-- throttling the querying of frames to -->
	  	<arg name="fps" value="15" />
	  	<!-- setting frame_id -->
	  	<arg name="frame_id" value="right_cam" />
	  	<!-- camera info loading, take care as it needs the "file:///" at the start , e.g.:
	  	"file:///$(find your_camera_package)/config/your_camera.yaml" -->
	  	<arg name="camera_info_url" value="" />
   </include>

   <include file="$(find video_stream_opencv)/launch/camera.launch" >
   		<!-- node name and ros graph name -->
	  	<arg name="camera_name" value="left_cam" />
	  	<!-- means video device 0, /dev/video0 -->
	  	<arg name="video_stream_provider" value="$(arg video_stream_provider_left)" />
	  	<!-- set camera fps to (if the device allows) -->
	  	<arg name="set_camera_fps" value="15"/>
      		<!-- set buffer queue size of frame capturing to (1 means we want the latest frame only) -->
      		<arg name="buffer_queue_size" value="1" />
	  	<!-- throttling the querying of frames to -->
	  	<arg name="fps" value="15" />
	  	<!-- setting frame_id -->
	  	<arg name="frame_id" value="left_cam" />
	  	<!-- camera info loading, take care as it needs the "file:///" at the start , e.g.:
	  	"file:///$(find your_camera_package)/config/your_camera.yaml" -->
	  	<arg name="camera_info_url" value="" />

   </include>
</launch>
